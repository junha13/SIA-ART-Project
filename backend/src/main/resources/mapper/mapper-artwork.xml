<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper-artwork">

    <insert id="insertArtwork" parameterType="lx.team2.backend.feature.product.ArtworkVO">
        INSERT INTO artwork (title, image_path, lat, lng)
        VALUES (#{title}, #{imagePath}, #{lat}, #{lng})
    </insert>

    <select id="selectAllArtworks" resultType="lx.team2.backend.feature.product.ArtworkVO">
        SELECT * FROM artwork
    </select>
</mapper>